
# ***** EMAIL *****

# How should email be sent out? We support smtp and mailgun
ACTION_MAILER_DELIVERY_METHOD=smtp

# In case ACTION_MAILER_DELIVERY_METHOD=mailgun
MAILGUN_API_KEY=
MAILGUN_DOMAIN=
# Optional, defaults to api.mailgun.net
# MAILGUN_API_HOST=

# In case ACTION_MAILER_DELIVERY_METHOD=smtp
# More information and default values can be found here:
# https://guides.rubyonrails.org/action_mailer_basics.html#action-mailer-configuration
SMTP_ADDRESS=
SMTP_PORT=
SMTP_DOMAIN=
SMTP_USER_NAME=
SMTP_PASSWORD=
SMTP_AUTHENTICATION=
SMTP_ENABLE_STARTTLS_AUTO=
SMTP_OPENSSL_VERIFY_MODE=
